{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "checkJs": false,
    "moduleSuffixes": [".ios", ".android", ".native", ""],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/env": ["./env.ts"]
    }
  },
  "include": ["src", "*.ts", "*.js", ".expo/types/**/*.ts", "expo-env.d.ts", "uniwind-types.d.ts"],
  "exclude": ["node_modules"]
}
